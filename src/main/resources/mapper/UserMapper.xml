<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.UserMapper">
    <select id="selectById" resultType="org.iicebreakker.domain.User">
            select *
            from t_user
            where id = #{id}
        </select>
    <select id="selectAll" resultType="org.iicebreakker.domain.User">
            select *
            from t_user
        </select>
</mapper>